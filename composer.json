{
    "name": "generalfocus/limitshield",
    "description": "LimitShield is a middleware package designed to manage and enforce API rate limiting efficiently.",
    "type": "library",
    "version": "1.2.0",
    "license": "MIT",
    "keywords": [
        "laravel",
        "middleware",
        "rate limiting",
        "API"
    ],
    "authors": [
        {
            "name": "Olabode Oyinkansola",
            "homepage": "https://x.com/generalfocus1",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "GeneralFocus\\LimitShield\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"

    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneralFocus\\LimitShield\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/GeneralFocus/LimitShield"
        }
    ],
    "support": {
        "issues": "https://github.com/GeneralFocus/LimitShield/issues"
    }
}